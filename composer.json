{
    "name": "evert/phpdoc-md",
    "description" : "PHP documentation generator, with markdown output",
    "keywords" : [ "PHP", "Markdown", "PHPDoc" ],
    "homepage" : "https://github.com/evert/phpdoc-md",
    "license" : "MIT",
    "require" : {
        "php" : ">=5.6",
        "twig/twig" : ">=1.2"
    },
    "require-dev" : {
        "phpdocumentor/phpdocumentor" : ">=2.9",
        "jms/serializer": "1.7.*",
        "phpunit/phpunit": ">=5.7"
    },
    "authors" : [
        {
            "name" : "Evert Pot",
            "email" : "evert@rooftopsolutions.nl",
            "homepage" : "http://www.rooftopsolutions.nl/",
            "role" : "Developer"
        },
        {
            "name" : "Jean Claveau",
            "role" : "Developer"
        }
    ],
    "autoload" : {
        "psr-4" : {
            "PHPDocMD\\" : "src/"
        }
    },
    "bin" : [
        "bin/phpdocmd"
    ],
    "config" : {
        "bin-dir" : "bin"
    }
}
